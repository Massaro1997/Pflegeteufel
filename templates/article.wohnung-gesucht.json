ground: #f8f9fa;\n        padding: 30px;\n        border-radius: 8px;\n        margin: 40px 0;\n        border: 1px solid #e9ecef;\n    }\n\n    .pflegeteufel-blog-wrapper .author-info {\n        display: flex;\n        align-items: center;\n        gap: 20px;\n    }\n\n    .pflegeteufel-blog-wrapper .author-avatar {\n        width: 60px;\n        height: 60px;\n        background: #C12624;\n        border-radius: 50%;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        color: white;\n        font-size: 24px;\n        flex-shrink: 0;\n    }\n\n    .pflegeteufel-blog-wrapper .author-details h4 {\n        font-size: 1.2rem;\n        color: #1a1a1a;\n        margin-bottom: 5px;\n    }\n\n    .pflegeteufel-blog-wrapper .author-details p {\n        font-size: 0.95rem;\n        color: #666;\n        margin: 0;\n    }\n\n    \/* Related articles *\/\n    .pflegeteufel-blog-wrapper .related-section {\n        background: #f8f9fa;\n        padding: 30px;\n        border-radius: 8px;\n        margin: 40px 0;\n        border: 1px solid #e9ecef;\n    }\n\n    .pflegeteufel-blog-wrapper .related-title {\n        font-size: 1.3rem;\n        color: #1a1a1a;\n        font-weight: 600;\n        margin-bottom: 20px;\n        text-align: center;\n    }\n\n    .pflegeteufel-blog-wrapper .related-article {\n        padding: 15px;\n        margin-bottom: 10px;\n        background: white;\n        border-radius: 4px;\n        border: 1px solid #e9ecef;\n        transition: box-shadow 0.2s ease;\n    }\n\n    .pflegeteufel-blog-wrapper .related-article:hover {\n        box-shadow: 0 2px 8px rgba(0,0,0,0.1);\n    }\n\n    .pflegeteufel-blog-wrapper .related-article a {\n        color: #333;\n        text-decoration: none;\n        font-weight: 500;\n    }\n\n    .pflegeteufel-blog-wrapper .related-article a:hover {\n        color: #C12624;\n    }\n\n    \/* Navigation *\/\n    .pflegeteufel-blog-wrapper .blog-navigation {\n        display: grid;\