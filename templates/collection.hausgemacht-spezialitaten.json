","icons_shape":"none","section_width":"page-width","horizontal_alignment":"center","gap":28,"color_scheme":"scheme-1","padding-block-start":0,"padding-block-end":48}},"section_xjzTNV":{"type":"section","blocks":{"custom_liquid_gGzYqJ":{"type":"custom-liquid","name":"t:names.custom_liquid","settings":{"custom_liquid":"<!-- PFLEGEBOX SEZIONE - DESIGN PULITO SENZA VERDE -->\n\n<div id=\"pflegebox-clean-container\"><\/div>\n\n<script>\n\/\/ Font Google Fonts\nif (!document.querySelector('link[href*=\"Inter\"]')) {\n    const fontLink = document.createElement('link');\n    fontLink.href = 'https:\/\/fonts.googleapis.com\/css2?family=Inter:wght@400;500;600;700;800&display=swap';\n    fontLink.rel = 'stylesheet';\n    document.head.appendChild(fontLink);\n}\n\n\/\/ Dati della sezione PFLEGEBOX\nvar pflegeboxInfo = [\n    {\n        id: 1,\n        icon: \"üì¶\",\n        title: \"WAS IST DIE PFLEGEBOX?\",\n        content: \"Die Pflegebox ist Ihr monatliches Paket mit hochwertigen Pflegehilfsmitteln zum Verbrauch. Individuell zusammengestellt nach Ihren Bed√ºrfnissen und vollst√§ndig von der Pflegekasse finanziert - bis zu 42‚Ç¨ jeden Monat kostenfrei f√ºr Sie.\"\n    },\n    {\n        id: 2,\n        icon: \"üè†\",\n        title: \"WER HAT ANSPRUCH?\",\n        content: \"Alle Personen mit anerkanntem Pflegegrad 1-5 haben laut ¬ß 40 SGB XI Anspruch auf kostenlose Pflegehilfsmittel. Die Pflege muss zu Hause stattfinden - durch Angeh√∂rige, Freunde oder professionelle Pflegedienste.\"\n    },\n    {\n        id: 3,\n        icon: \"üß§\",\n        title: \"WAS IST ENTHALTEN?\",\n        content: \"Einmalhandschuhe in verschiedenen Gr√∂√üen, Desinfektionsmittel f√ºr H√§nde und Fl√§chen, Mundschutzmasken, Schutzsch√ºrzen, Bettschutzeinlagen und weitere Hilfsmittel - alles perfekt auf Ihre Pflegesituation abgestimmt.\"\n    },\n    {\n        id: 4,\n        icon: \"üöö\",\n        title: \"WIE L√ÑUFT ES AB?\",\n        content: \"Nach einmaliger Anmeldung erhalten Sie automatisch jeden Monat Ihre pers√∂nliche Pflegebox direkt vor die Haust√ºr. Wir √ºbernehmen Antragstellung, Abrechnung mit der Pflegekasse und alle Formalit√§ten - komplett kostenlos.\"\n    }\n];\n\n\/\/ JavaScript per creare la sezione\nfunction createCleanPflegeboxSection() {\n    const container = document.getElementById('pflegebox-clean-container');\n    \n    if (!container) {\n        console.error('Container non trovato');\n        return;\n    }\n    \n    \/\/ Crea l'HTML della sezione\n    const sectionHTML = `\n        <div class=\"pflegebox-clean-section\" style=\"padding: 20px 0 40px 0; background: white; min-height: 100vh; font-family: 'Inter', sans-serif;\">\n            <div class=\"pflegebox-clean-container\" style=\"max-width: 1400px; margin: 0 auto; padding: 0 20px; width: 100%;\">\n                \n                <!-- Header della sezione -->\n                <div class=\"pflegebox-clean-header\" id=\"pflegebox-clean-header\" style=\"text-align: center; margin-bottom: 40px; opacity: 0; transform: translateY(30px); transition: all 1.2s ease;\">\n                    <h2 class=\"pflegebox-clean-title\" style=\"font-size: 2.2rem; font-weight: 700; color: #2c3e50; margin-bottom: 12px; line-height: 1.2;\">\n                        DIE PFLEGEBOX ‚Äì IHR MONATLICHES PAKET\n                    <\/h2>\n                    <p class=\"pflegebox-clean-subtitle\" style=\"font-size: 1.1rem; color: #6c757d; max-width: 700px; margin: 0 auto; line-height: 1.4;\">\n                        Hochwertige Pflegehilfsmittel direkt zu Ihnen nach Hause. Von der Pflegekasse bezahlt, jeden Monat kostenfrei bis zu 42‚Ç¨.\n                    <\/p>\n                <\/div>\n\n                <!-- Info Grid -->\n                <div class=\"pflegebox-clean-grid\" id=\"pflegebox-clean-grid\" style=\"display: grid; grid-template-columns: repeat(4, 1fr); gap: 20px;\">\n                    ${pflegeboxInfo.map((info, index) => createCleanInfoCard(info, index)).join('')}\n                <\/div>\n            <\/div>\n        <\/div>\n    `;\n    \n    \/\/ Inserisci l'HTML nel container\n    container.innerHTML = sectionHTML;\n    \n    \/\/ Aggiungi CSS dinamicamente\n    addCleanStyles();\n    \n    \/\/ Aggiungi animazioni\n    addCleanIntersectionObserver();\n    \n    console.log('‚úÖ Sezione Pflegebox pulita creata con successo!');\n}\n\n\/\/ Funzione per creare le card informative\nfunction createCleanInfoCard(info, index) {\n    return `\n        <div class=\"pflegebox-clean-item\" style=\"background: white; border-radius: 8px; padding: 25px 20px; box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1); transition: all 0.6s cubic-bezier(0.175, 0.885, 0.32, 1.275); border: 1px solid #e9ecef; display: flex; flex-direction: column; height: 100%; opacity: 0; transform: translateY(50px);\">\n            <div class=\"pflegebox-clean-icon\" style=\"background: #f8f9fa; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 20px; border: 2px solid #C12624; transition: all 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275); font-size: 28px;\">\n                ${info.icon}\n            <\/div>\n            <h3 class=\"pflegebox-clean-card-title\" style=\"font-size: 1.1rem; font-weight: 700; color: #2c3e50; margin-bottom: 15px; text-align: center; line-height: 1.2;\">\n                ${info.title}\n            <\/h3>\n            <div class=\"pflegebox-clean-content\" style=\"flex-grow: 1; display: flex; align-items: center;\">\n                <p class=\"pflegebox-clean-text\" style=\"color: #555; line-height: 1.6; margin: 0; font-size: 0.95rem; text-align: center;\">\n                    ${info.content}\n                <\/p>\n            <\/div>\n        <\/div>\n    `;\n}\n\n\/\/ Funzione per aggiungere gli stili CSS\nfunction addCleanStyles() {\n    if (document.getElementById('pflegebox-clean-styles')) return;\n    \n    const styles = document.createElement('style');\n    styles.id = 'pflegebox-clean-styles';\n    styles.innerHTML = `\n        \/* Hover effects *\/\n        .pflegebox-clean-item:hover {\n            transform: translateY(-8px);\n            box-shadow: 0 8px 25px rgba(0, 0, 0, 0.15);\n        }\n        \n        .pflegebox-clean-item:hover .pflegebox-clean-icon {\n            transform: scale(1.1);\n            border-color: #A01F1D;\n            box-shadow: 0 5px 15px rgba(193, 38, 36, 0.3);\n        }\n\n        \/* Animazioni classe .animate per Intersection Observer *\/\n        .pflegebox-clean-header.animate {\n            opacity: 1 !important;\n            transform: translateY(0) !important;\n        }\n        \n        .pflegebox-clean-item.animate {\n            opacity: 1 !important;\n            transform: translateY(0) !important;\n        }\n\n        \/* Responsive *\/\n        @media (max-width: 1200px) {\n            .pflegebox-clean-grid {\n                grid-template-columns: repeat(2, 1fr) !important;\n                gap: 15px !important;\n            }\n        }\n        \n        @media (max-width: 767px) {\n            .pflegebox-clean-grid {\n                grid-template-columns: 1fr !important;\n            }\n            \n            .pflegebox-clean-title {\n                font-size: 1.8rem !important;\n            }\n            \n            .pflegebox-clean-item {\n                padding: 20px 15px !important;\n            }\n        }\n    `;\n    \n    document.head.appendChild(styles);\n}\n\n\/\/ Funzione per aggiungere animazioni con Intersection Observer\nfunction addCleanIntersectionObserver() {\n    const observer = new IntersectionObserver(function(entries) {\n        entries.forEach(function(entry) {\n            if (entry.isIntersecting) {\n                entry.target.classList.add('animate');\n            }\n        });\n    }, { \n        threshold: 0.1,\n        rootMargin: '0px 0px -50px 0px'\n    });\n\n    \/\/ Aspetta che gli elementi siano renderizzati\n    setTimeout(() => {\n        \/\/ Anima l'header\n        const header = document.getElementById('pflegebox-clean-header');\n        if (header) {\n            observer.observe(header);\n        }\n\n        \/\/ Anima le cards con delay sequenziale\n        const cleanItems = document.querySelectorAll('.pflegebox-clean-item');\n        cleanItems.forEach((item, index) => {\n            item.style.transitionDelay = `${0.2 + (index * 0.15)}s`;\n            observer.observe(item);\n        });\n        \n        console.log('‚ú® Pflegebox Clean Intersection Observer attivato!');\n    }, 100);\n}\n\n\/\/ Auto-inizializzazione quando il DOM √® pronto\ndocument.addEventListener('DOMContentLoaded', function() {\n    createCleanPflegeboxSection();\n});\n\n\/\/ Se il DOM √® gi√† caricato, esegui subito\nif (document.readyState === 'loading') {\n    document.addEventListener('DOMContentLoaded', createCleanPflegeboxSection);\n} else {\n    createCleanPflegeboxSection();\n}\n\n\/\/ Esporta le funzioni per uso esterno\nif (typeof window !== 'undefined') {\n    window.CleanPflegeboxSection = {\n        create: createCleanPflegeboxSection\n    };\n}\n<\/script>"},"blocks":{}}},"block_order":["custom_liquid_gGzYqJ"],"name":"t:names.custom_section","settings":{"content_direction":"column","vertical_on_mobile":true,"horizontal_alignment":"flex-start","vertical_alignment":"center","align_baseline":false,"horizontal_alignment_flex_direction_column":"center","vertical_alignment_flex_direction_column":"center","gap":0,"section_width":"page-width","section_height":"","section_height_custom":50,"color_scheme":"scheme-1","background_media":"none","video_position":"cover","background_image_position":"cover","border":"none","border_width":1,"border_opacity":100,"border_radius":0,"toggle_overlay":false,"overlay_color":"#00000026","overlay_style":"solid","gradient_direction":"to top","padding-block-start":0,"padding-block-end":0}},"main":{"type":"main-collection","blocks":{"filters":{"type":"filters","static":