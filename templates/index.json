            "custom_liquid": "<!DOCTYPE html>\n<html lang=\"de\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Support Section - Clean</title>\n    <style>\n        /* Reset */\n        .support-wrapper {\n            position: relative;\n            z-index: 3;\n            isolation: isolate;\n        }\n        \n        .support-wrapper * {\n            box-sizing: border-box;\n        }\n\n        .support-section {\n            padding: 40px 0;\n            background: #F8F8F8;\n            font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;\n        }\n        \n        .support-container {\n            max-width: 1200px;\n            margin: 0 auto;\n            padding: 0 20px;\n        }\n        \n        /* Header */\n        .support-header {\n            text-align: center;\n            margin-bottom: 40px;\n        }\n        \n        .support-title {\n            font-size: 2rem;\n            font-weight: 700;\n            color: #2c3e50;\n            margin-bottom: 12px;\n        }\n        \n        .support-subtitle {\n            font-size: 1.1rem;\n            color: #6c757d;\n            max-width: 600px;\n            margin: 0 auto;\n        }\n        \n        /* Grid */\n        .support-grid {\n            display: grid;\n            grid-template-columns: repeat(4, 1fr);\n            gap: 20px;\n            margin-bottom: 30px;\n        }\n        \n        /* Card */\n        .support-card {\n            background: white;\n            border-radius: 12px;\n            padding: 30px 20px;\n            box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n            transition: all 0.3s ease;\n            border: 2px solid transparent;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            text-align: center;\n            height: 100%;\n            cursor: pointer;\n            position: relative;\n        }\n        \n        .support-card:hover {\n            transform: translateY(-5px);\n            box-shadow: 0 8px 25px rgba(0, 0, 0, 0.15);\n        }\n        \n        .support-card.active {\n            border-color: #C12624;\n            transform: translateY(-5px);\n            box-shadow: 0 8px 25px rgba(193, 38, 36, 0.2);\n        }\n        \n        /* Icona con immagine */\n        .support-icon {\n            background: #f8f9fa;\n            width: 200px;\n            height: 200px;\n            border-radius: 50%;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin: 0 auto 15px;\n            border: 3px solid #C12624;\n            transition: all 0.3s ease;\n            overflow: hidden;\n            position: relative;\n        }\n\n        .support-icon img {\n            width: 190px;\n            height: 190px;\n            border-radius: 50%;\n            object-fit: cover;\n            transition: all 0.3s ease;\n        }\n        \n        .support-card:hover .support-icon,\n        .support-card.active .support-icon {\n            transform: scale(1.1);\n            border-color: #A01F1D;\n            box-shadow: 0 5px 15px rgba(193, 38, 36, 0.3);\n        }\n\n        .support-card:hover .support-icon img,\n        .support-card.active .support-icon img {\n            transform: scale(1.05);\n        }\n        \n        /* Titolo */\n        .support-card-title {\n            font-size: 1.1rem;\n            font-weight: 700;\n            color: #2c3e50;\n            margin-bottom: 15px;\n            line-height: 1.3;\n            flex-grow: 1;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n        }\n        \n        /* Bottone */\n        .support-btn {\n            background: linear-gradient(45deg, #C12624, #A01F1D);\n            color: white;\n            border: none;\n            padding: 12px 25px;\n            border-radius: 25px;\n            font-size: 0.9rem;\n            font-weight: 600;\n            cursor: pointer;\n            transition: all 0.3s ease;\n            text-transform: uppercase;\n            letter-spacing: 0.5px;\n            position: relative;\n            overflow: hidden;\n            box-shadow: 0 4px 15px rgba(193, 38, 36, 0.2);\n        }\n        \n        .support-btn::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: -100%;\n            width: 100%;\n            height: 100%;\n            background: linear-gradient(90deg, transparent, rgba(255,255,255,0.3), transparent);\n            transition: left 0.5s;\n        }\n        \n        .support-card:hover .support-btn::before {\n            left: 100%;\n        }\n        \n        .support-card:hover .support-btn,\n        .support-card.active .support-btn {\n            transform: translateY(-2px) scale(1.05);\n            box-shadow: 0 6px 20px rgba(193, 38, 36, 0.4);\n        }\n        \n        /* Contenuto espandibile DESKTOP - sotto la griglia */\n        .support-details-container {\n            width: 100%;\n            margin-top: 20px;\n        }\n        \n        .support-details {\n            background: white;\n            border-radius: 12px;\n            border: 1px solid #e9ecef;\n            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.08);\n            max-height: 0;\n            overflow: hidden;\n            transition: all 0.4s ease;\n            opacity: 0;\n            transform: translateY(-10px);\n        }\n        \n        .support-details.show {\n            max-height: 400px;\n            opacity: 1;\n            transform: translateY(0);\n        }\n        \n        .support-details-header {\n            background: #f8f9fa;\n            border-bottom: 1px solid #e9ecef;\n            padding: 20px;\n            border-radius: 12px 12px 0 0;\n            display: flex;\n            align-items: center;\n            gap: 15px;\n        }\n        \n        .support-details-icon {\n            width: 70px;\n            height: 70px;\n            border-radius: 50%;\n            overflow: hidden;\n            border: 2px solid #C12624;\n        }\n\n        .support-details-icon img {\n            width: 100%;\n            height: 100%;\n            object-fit: cover;\n        }\n        \n        .support-details-title {\n            font-size: 1.1rem;\n            font-weight: 600;\n            margin: 0;\n            color: #2c3e50;\n        }\n        \n        .support-problem {\n            background: white;\n            padding: 20px;\n            border-left: 3px solid #ff6b6b;\n            margin: 0;\n            position: relative;\n        }\n        \n        .support-problem::before {\n            content: '⚠️';\n            position: absolute;\n            top: 15px;\n            right: 20px;\n            font-size: 1.2rem;\n            opacity: 0.6;\n        }\n        \n        .support-solution {\n            background: white;\n            padding: 20px;\n            border-left: 3px solid #28a745;\n            border-radius: 0 0 12px 12px;\n            position: relative;\n            border-top: 1px solid #f1f3f4;\n        }\n        \n        .support-solution::before {\n            content: '✅';\n            position: absolute;\n            top: 15px;\n            right: 20px;\n            font-size: 1.2rem;\n            opacity: 0.6;\n        }\n        \n        .support-details h4 {\n            margin: 0 0 10px 0;\n            font-size: 1rem;\n            font-weight: 600;\n            color: #6c757d;\n            text-transform: uppercase;\n            letter-spacing: 0.3px;\n        }\n        \n        .support-problem h4 {\n            color: #dc3545;\n        }\n        \n        .support-solution h4 {\n            color: #28a745;\n        }\n        \n        .support-details p {\n            color: #495057;\n            line-height: 1.6;\n            margin: 0;\n            font-size: 0.95rem;\n            font-weight: 400;\n        }\n        \n        /* Contenuto espandibile MOBILE - dentro le card */\n        .support-details-mobile {\n            width: 100%;\n            margin-top: 20px;\n            max-height: 0;\n            overflow: hidden;\n            transition: all 0.4s ease;\n            opacity: 0;\n        }\n        \n        .support-details-mobile.show {\n            max-height: 500px;\n            opacity: 1;\n        }\n        \n        .support-details-mobile .support-problem {\n            background: #fff5f5;\n            padding: 15px;\n            border-radius: 8px;\n            border-left: 3px solid #ff6b6b;\n            margin-bottom: 15px;\n            position: relative;\n        }\n        \n        .support-details-mobile .support-problem::before {\n            content: '⚠️';\n            position: absolute;\n            top: 10px;\n            right: 15px;\n            font-size: 1rem;\n            opacity: 0.7;\n        }\n        \n        .support-details-mobile .support-solution {\n            background: #f0f8f0;\n            padding: 15px;\n            border-radius: 8px;\n            border-left: 3px solid #28a745;\n            position: relative;\n            border-top: none;\n        }\n        \n        .support-details-mobile .support-solution::before {\n            content: '✅';\n            position: absolute;\n            top: 10px;\n            right: 15px;\n            font-size: 1rem;\n            opacity: 0.7;\n        }\n        \n        .support-details-mobile h4 {\n            margin: 0 0 8px 0;\n            font-size: 0.9rem;\n            font-weight: 600;\n            text-transform: uppercase;\n            letter-spacing: 0.3px;\n        }\n        \n        .support-details-mobile .support-problem h4 {\n            color: #dc3545;\n        }\n        \n        .support-details-mobile .support-solution h4 {\n            color: #28a745;\n        }\n        \n        .support-details-mobile p {\n            color: #495057;\n            line-height: 1.5;\n            margin: 0;\n            font-size: 0.85rem;\n            font-weight: 400;\n        }\n        \n        /* CTA */\n        .support-cta {\n            text-align: center;\n            margin-top: 40px;\n        }\n        \n        .support-cta-btn {\n            display: inline-block;\n            background: #C12624;\n            color: white;\n            padding: 15px 30px;\n            font-size: 1rem;\n            font-weight: 600;\n            text-decoration: none;\n            border-radius: 8px;\n            transition: all 0.3s ease;\n        }\n        \n        .support-cta-btn:hover {\n            background: #A01F1D;\n            transform: translateY(-2px);\n            box-shadow: 0 5px 15px rgba(193, 38, 36, 0.3);\n        }\n        \n        /* Responsive */\n        @media (max-width: 1200px) {\n            .support-grid {\n                grid-template-columns: repeat(2, 1fr);\n                gap: 15px;\n            }\n        }\n        \n        @media (max-width: 768px) {\n            .support-grid {\n                grid-template-columns: 1fr;\n            }\n            \n            .support-title {\n                font-size: 1.8rem;\n            }\n            \n            .support-card {\n                padding: 20px 15px;\n                height: auto;\n            }\n            \n            .support-icon {\n                width: 165px;\n                height: 165px;\n                margin: 0 auto 10px;\n            }\n\n            .support-icon img {\n                width: 155px;\n                height: 155px;\n            }\n            \n            /* Su mobile: nascondi solo la sezione desktop */\n            .support-details-container {\n                display: none;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"support-wrapper\">\n        <section class=\"support-section\">\n            <div class=\"support-container\">\n                <!-- Header -->\n                <div class=\"support-header\">\n                    <h2 class=\"support-title\">WEN UNTERSTÜTZEN WIR – UND WIE?</h2>\n                    <p class=\"support-subtitle\">Wir sind da, um euch zu helfen – und das in eurer Sprache.</p>\n                </div>\n\n                <!-- Grid -->\n                <div class=\"support-grid\">\n                    <div class=\"support-card\" data-id=\"1\" data-image=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/1.jpg?v=1749999444\" data-title=\"FAMILIEN MIT PFLEGEBEDÜRFTIGEN ANGEHÖRIGEN\">\n                        <div class=\"support-icon\">\n                            <img src=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/1.jpg?v=1749999444\" alt=\"Familie mit pflegebedürftigen Angehörigen\" loading=\"lazy\">\n                        </div>\n                        <h3 class=\"support-card-title\">FAMILIEN MIT PFLEGEBEDÜRFTIGEN ANGEHÖRIGEN</h3>\n                        <button class=\"support-btn\">Mehr erfahren</button>\n                        \n                        <div class=\"support-details-mobile\">\n                            <div class=\"support-problem\">\n                                <h4>Das Problem:</h4>\n                                <p>Ein Elternteil oder Großelternteil wird pflegebedürftig, doch niemand in der Familie weiß, wie man einen Antrag stellt, welche Leistungen verfügbar sind oder was der Pflegegrad genau bedeutet. Die Bürokratie wirkt überwältigend.</p>\n                            </div>\n                            <div class=\"support-solution\">\n                                <h4>Unsere Lösung:</h4>\n                                <p>Wir übernehmen die komplette Antragstellung für den Pflegegrad, erklären jeden Schritt verständlich auf Italienisch und helfen bei der gesamten Kommunikation mit den Pflegekassen. Auch bei Widersprüchen stehen wir fest an deiner Seite.</p>\n                            </div>\n                        </div>\n                    </div>\n\n                    <div class=\"support-card\" data-id=\"2\" data-image=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/2.png?v=1749999445\" data-title=\"MENSCHEN MIT BEHINDERUNG ODER CHRONISCHEN KRANKHEITEN\">\n                        <div class=\"support-icon\">\n                            <img src=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/2.png?v=1749999445\" alt=\"Menschen mit Behinderung oder chronischen Krankheiten\" loading=\"lazy\">\n                        </div>\n                        <h3 class=\"support-card-title\">MENSCHEN MIT BEHINDERUNG ODER CHRONISCHEN KRANKHEITEN</h3>\n                        <button class=\"support-btn\">Mehr erfahren</button>\n                        \n                        <div class=\"support-details-mobile\">\n                            <div class=\"support-problem\">\n                                <h4>Das Problem:</h4>\n                                <p>Viele Betroffene kennen ihre Rechte nicht und verlieren dadurch Geld oder wichtige Leistungen, weil sie Anträge falsch oder gar nicht stellen. Sprachprobleme machen die Situation noch komplizierter und frustrierender.</p>\n                            </div>\n                            <div class=\"support-solution\">\n                                <h4>Unsere Lösung:</h4>\n                                <p>Wir erklären deine Möglichkeiten und Rechte ausführlich, stellen alle richtigen Anträge wie Pflegegrad, Schwerbehindertenausweis oder Hilfsmittel und begleiten dich durch den gesamten Prozess – kostenlos und professionell auf Augenhöhe.</p>\n                            </div>\n                        </div>\n                    </div>\n\n                    <div class=\"support-card\" data-id=\"3\" data-image=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/3.jpg?v=1749999444\" data-title=\"GESETZLICHE BETREUER ODER BEVOLLMÄCHTIGTE\">\n                        <div class=\"support-icon\">\n                            <img src=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/3.jpg?v=1749999444\" alt=\"Gesetzliche Betreuer oder Bevollmächtigte\" loading=\"lazy\">\n                        </div>\n                        <h3 class=\"support-card-title\">GESETZLICHE BETREUER ODER BEVOLLMÄCHTIGTE</h3>\n                        <button class=\"support-btn\">Mehr erfahren</button>\n                        \n                        <div class=\"support-details-mobile\">\n                            <div class=\"support-problem\">\n                                <h4>Das Problem:</h4>\n                                <p>Du kümmerst dich um eine andere Person, bist aber völlig überfordert mit den vielen Formularen, strengen Fristen und komplexen Behördengängen. Oft fehlt dir einfach die Zeit oder das spezielle Wissen für die richtige Vorgehensweise.</p>\n                            </div>\n                            <div class=\"support-solution\">\n                                <h4>Unsere Lösung:</h4>\n                                <p>PflegeTeufel wird dein verlässlicher Partner im Hintergrund: Wir analysieren gründlich den Fall, bereiten alle wichtigen Unterlagen professionell vor, unterstützen dich bei allen Terminen und sorgen dafür, dass optimale Leistungen beantragt werden.</p>\n                            </div>\n                        </div>\n                    </div>\n\n                    <div class=\"support-card\" data-id=\"4\" data-image=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/09_leben_-_Copia.jpg?v=1750000203\" data-title=\"MENSCHEN MIT PFLEGEGRAD-ABLEHNUNG\">\n                        <div class=\"support-icon\">\n                            <img src=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/09_leben_-_Copia.jpg?v=1750000203\" alt=\"Menschen mit Pflegegrad-Ablehnung\" loading=\"lazy\">\n                        </div>\n                        <h3 class=\"support-card-title\">MENSCHEN MIT PFLEGEGRAD-ABLEHNUNG</h3>\n                        <button class=\"support-btn\">Mehr erfahren</button>\n                        \n                        <div class=\"support-details-mobile\">\n                            <div class=\"support-problem\">\n                                <h4>Das Problem:</h4>\n                                <p>Der Pflegegrad wurde abgelehnt – obwohl für jeden klar ersichtlich ist, dass dringend Hilfe und Unterstützung nötig ist. Viele Betroffene wissen leider nicht, dass ein Widerspruch durchaus möglich und oft erfolgreich ist.</p>\n                            </div>\n                            <div class=\"support-solution\">\n                                <h4>Unsere Lösung:</h4>\n                                <p>Wir prüfen deinen Bescheid gründlich und rechtlich fundiert, schreiben den Widerspruch professionell und mit allen notwendigen medizinischen Argumenten und begleiten dich persönlich zur Neubewertung für den bestmöglichen Erfolg.</p>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n\n                <!-- Contenuto espandibile DESKTOP - sotto la griglia -->\n                <div class=\"support-details-container\">\n                    <div class=\"support-details\" id=\"supportDetails\">\n                        <div class=\"support-details-header\">\n                            <div class=\"support-details-icon\" id=\"detailsIcon\">\n                                <img id=\"detailsImage\" src=\"\" alt=\"\" loading=\"lazy\">\n                            </div>\n                            <h3 class=\"support-details-title\" id=\"detailsTitle\"></h3>\n                        </div>\n                        <div class=\"support-problem\">\n                            <h4>Das Problem:</h4>\n                            <p id=\"problemText\"></p>\n                        </div>\n                        <div class=\"support-solution\">\n                            <h4>Unsere Lösung:</h4>\n                            <p id=\"solutionText\"></p>\n                        </div>\n                    </div>\n                </div>\n\n                <!-- CTA -->\n                <div class=\"support-cta\">\n                    <a href=\"https://0phy01-hc.myshopify.com/pages/kontakt-uns\" class=\"support-cta-btn\">\n                        Kontaktiere uns jetzt\n                    </a>\n                </div>\n            </div>\n        </section>\n    </div>\n\n    <script>\n        // Dati dei servizi\n        const supportData = {\n            1: {\n                problem: \"Ein Elternteil oder Großelternteil wird pflegebedürftig, doch niemand in der Familie weiß, wie man einen Antrag stellt, welche Leistungen verfügbar sind oder was der Pflegegrad genau bedeutet. Die Bürokratie wirkt überwältigend.\",\n                solution: \"Wir übernehmen die komplette Antragstellung für den Pflegegrad, erklären jeden Schritt verständlich auf Italienisch und helfen bei der gesamten Kommunikation mit den Pflegekassen. Auch bei Widersprüchen stehen wir fest an deiner Seite.\"\n            },\n            2: {\n                problem: \"Viele Betroffene kennen ihre Rechte nicht und verlieren dadurch Geld oder wichtige Leistungen, weil sie Anträge falsch oder gar nicht stellen. Sprachprobleme machen die Situation noch komplizierter und frustrierender.\",\n                solution: \"Wir erklären deine Möglichkeiten und Rechte ausführlich, stellen alle richtigen Anträge wie Pflegegrad, Schwerbehindertenausweis oder Hilfsmittel und begleiten dich durch den gesamten Prozess – kostenlos und professionell auf Augenhöhe.\"\n            },\n            3: {\n                problem: \"Du kümmerst dich um eine andere Person, bist aber völlig überfordert mit den vielen Formularen, strengen Fristen und komplexen Behördengängen. Oft fehlt dir einfach die Zeit oder das spezielle Wissen für die richtige Vorgehensweise.\",\n                solution: \"PflegeTeufel wird dein verlässlicher Partner im Hintergrund: Wir analysieren gründlich den Fall, bereiten alle wichtigen Unterlagen professionell vor, unterstützen dich bei allen Terminen und sorgen dafür, dass optimale Leistungen beantragt werden.\"\n            },\n            4: {\n                problem: \"Der Pflegegrad wurde abgelehnt – obwohl für jeden klar ersichtlich ist, dass dringend Hilfe und Unterstützung nötig ist. Viele Betroffene wissen leider nicht, dass ein Widerspruch durchaus möglich und oft erfolgreich ist.\",\n                solution: \"Wir prüfen deinen Bescheid gründlich und rechtlich fundiert, schreiben den Widerspruch professionell und mit allen notwendigen medizinischen Argumenten und begleiten dich persönlich zur Neubewertung für den bestmöglichen Erfolg.\"\n            }\n        };\n\n        // JavaScript pulito e funzionante\n        document.addEventListener('DOMContentLoaded', function() {\n            const cards = document.querySelectorAll('.support-card');\n            const desktopDetails = document.getElementById('supportDetails');\n            const detailsIcon = document.getElementById('detailsIcon');\n            const detailsImage = document.getElementById('detailsImage');\n            const detailsTitle = document.getElementById('detailsTitle');\n            const problemText = document.getElementById('problemText');\n            const solutionText = document.getElementById('solutionText');\n            \n            let activeCard = null;\n            \n            function isMobile() {\n                return window.innerWidth <= 768;\n            }\n            \n            cards.forEach(card => {\n                const btn = card.querySelector('.support-btn');\n                const cardId = parseInt(card.getAttribute('data-id'));\n                const cardImage = card.getAttribute('data-image');\n                const cardTitle = card.getAttribute('data-title');\n                const mobileDetails = card.querySelector('.support-details-mobile');\n                \n                btn.addEventListener('click', function(e) {\n                    e.preventDefault();\n                    e.stopPropagation();\n                    \n                    if (isMobile()) {\n                        // MOBILE: espansione dentro la card\n                        cards.forEach(c => {\n                            if (c !== card) {\n                                c.classList.remove('active');\n                                const otherMobile = c.querySelector('.support-details-mobile');\n                                if (otherMobile) otherMobile.classList.remove('show');\n                                c.querySelector('.support-btn').textContent = 'Mehr erfahren';\n                            }\n                        });\n                        \n                        if (card.classList.contains('active')) {\n                            card.classList.remove('active');\n                            mobileDetails.classList.remove('show');\n                            btn.textContent = 'Mehr erfahren';\n                        } else {\n                            card.classList.add('active');\n                            mobileDetails.classList.add('show');\n                            btn.textContent = 'Weniger anzeigen';\n                        }\n                        \n                    } else {\n                        // DESKTOP: espansione sotto la griglia\n                        if (activeCard === card) {\n                            card.classList.remove('active');\n                            desktopDetails.classList.remove('show');\n                            btn.textContent = 'Mehr erfahren';\n                            activeCard = null;\n                            return;\n                        }\n                        \n                        cards.forEach(c => {\n                            c.classList.remove('active');\n                            c.querySelector('.support-btn').textContent = 'Mehr erfahren';\n                        });\n                        \n                        card.classList.add('active');\n                        btn.textContent = 'Weniger anzeigen';\n                        activeCard = card;\n                        \n                        const data = supportData[cardId];\n                        detailsImage.src = cardImage;\n                        detailsImage.alt = cardTitle;\n                        detailsTitle.textContent = cardTitle;\n                        problemText.textContent = data.problem;\n                        solutionText.textContent = data.solution;\n                        \n                        desktopDetails.classList.add('show');\n                        \n                        setTimeout(() => {\n                            desktopDetails.scrollIntoView({ \n                                behavior: 'smooth', \n                                block: 'nearest' \n                            });\n                        }, 300);\n                    }\n                });\n            });\n        });\n    </script>\n</body>\n</html>"
          },
          "blocks": {}
        }
