    "custom_liquid_ixPyhP": {
      "type": "custom-liquid",
      "name": "t:names.custom_liquid",
      "settings": {
        "custom_liquid": "<!-- SEZIONE VIDEO YOUTUBE PFLEGETEUFEL - PRONTA ALL'USO -->\n\n<style>\n  .youtube-section {\n    width: 100%;\n    padding: 60px 20px;\n    background: linear-gradient(135deg, #f5f5f5 0%, #e8e8e8 100%);\n  }\n\n  .youtube-container {\n    max-width: 1200px;\n    margin: 0 auto;\n  }\n\n  .youtube-header {\n    text-align: center;\n    margin-bottom: 50px;\n  }\n\n  .youtube-title {\n    font-size: 42px;\n    font-weight: 900;\n    color: #000;\n    margin-bottom: 15px;\n    text-transform: uppercase;\n    letter-spacing: -1px;\n  }\n\n  .youtube-subtitle {\n    font-size: 18px;\n    color: #666;\n    margin-bottom: 25px;\n  }\n\n  .youtube-channel-link {\n    display: inline-flex;\n    align-items: center;\n    gap: 10px;\n    background: linear-gradient(135deg, #C12624, #A01F1D);\n    color: white;\n    padding: 12px 25px;\n    border-radius: 50px;\n    text-decoration: none;\n    font-weight: 600;\n    transition: all 0.3s ease;\n    box-shadow: 0 4px 15px rgba(193, 38, 36, 0.4);\n  }\n\n  .youtube-channel-link:hover {\n    transform: translateY(-3px);\n    box-shadow: 0 6px 20px rgba(193, 38, 36, 0.6);\n    color: white;\n  }\n\n  .youtube-icon {\n    width: 24px;\n    height: 24px;\n  }\n\n  .youtube-videos-grid {\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));\n    gap: 30px;\n    margin-top: 40px;\n  }\n\n  .youtube-video-card {\n    background: white;\n    border-radius: 15px;\n    overflow: hidden;\n    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\n    transition: all 0.3s ease;\n    cursor: pointer;\n  }\n\n  .youtube-video-card:hover {\n    transform: translateY(-8px);\n    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.15);\n  }\n\n  .youtube-video-thumbnail {\n    position: relative;\n    width: 100%;\n    padding-bottom: 56.25%; /* 16:9 aspect ratio */\n    overflow: hidden;\n    background: #000;\n  }\n\n  .youtube-video-thumbnail img {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n    transition: transform 0.3s ease;\n  }\n\n  .youtube-video-card:hover .youtube-video-thumbnail img {\n    transform: scale(1.05);\n  }\n\n  .youtube-play-overlay {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    width: 70px;\n    height: 70px;\n    background: rgba(193, 38, 36, 0.95);\n    border-radius: 50%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    transition: all 0.3s ease;\n    z-index: 2;\n  }\n\n  .youtube-video-card:hover .youtube-play-overlay {\n    background: rgba(193, 38, 36, 1);\n    transform: translate(-50%, -50%) scale(1.15);\n  }\n\n  .youtube-play-overlay svg {\n    width: 30px;\n    height: 30px;\n    fill: white;\n    margin-left: 4px;\n  }\n\n  .youtube-video-info {\n    padding: 20px;\n  }\n\n  .youtube-video-title {\n    font-size: 16px;\n    font-weight: 700;\n    color: #000;\n    margin-bottom: 10px;\n    line-height: 1.4;\n    display: -webkit-box;\n    -webkit-line-clamp: 2;\n    -webkit-box-orient: vertical;\n    overflow: hidden;\n  }\n\n  .youtube-video-meta {\n    display: flex;\n    justify-content: space-between;\n    font-size: 13px;\n    color: #888;\n  }\n\n  .youtube-loading {\n    text-align: center;\n    padding: 60px 20px;\n    font-size: 18px;\n    color: #666;\n  }\n\n  .youtube-loading::after {\n    content: '...';\n    animation: dots 1.5s steps(4, end) infinite;\n  }\n\n  @keyframes dots {\n    0%, 20% { content: '.'; }\n    40% { content: '..'; }\n    60%, 100% { content: '...'; }\n  }\n\n  .youtube-error {\n    text-align: center;\n    padding: 40px 20px;\n    background: #ffe6e6;\n    border-radius: 10px;\n    color: #c12624;\n    font-size: 16px;\n  }\n\n  /* Modal/Lightbox per video */\n  .youtube-modal {\n    display: none;\n    position: fixed;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    background: rgba(0, 0, 0, 0.95);\n    z-index: 9999;\n    align-items: center;\n    justify-content: center;\n    animation: fadeIn 0.3s ease;\n  }\n\n  .youtube-modal.active {\n    display: flex;\n  }\n\n  @keyframes fadeIn {\n    from { opacity: 0; }\n    to { opacity: 1; }\n  }\n\n  .youtube-modal-content {\n    position: relative;\n    width: 90%;\n    max-width: 1200px;\n    aspect-ratio: 16 / 9;\n    background: #000;\n    border-radius: 10px;\n    overflow: hidden;\n    animation: slideUp 0.4s ease;\n  }\n\n  @keyframes slideUp {\n    from {\n      opacity: 0;\n      transform: translateY(50px);\n    }\n    to {\n      opacity: 1;\n      transform: translateY(0);\n    }\n  }\n\n  .youtube-modal-iframe {\n    width: 100%;\n    height: 100%;\n    border: none;\n  }\n\n  .youtube-modal-close {\n    position: absolute;\n    top: -50px;\n    right: 0;\n    background: transparent;\n    border: none;\n    color: white;\n    font-size: 40px;\n    cursor: pointer;\n    width: 50px;\n    height: 50px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    transition: all 0.3s ease;\n    z-index: 10000;\n  }\n\n  .youtube-modal-close:hover {\n    transform: rotate(90deg);\n    color: #C12624;\n  }\n\n  @media (max-width: 768px) {\n    .youtube-section {\n      padding: 40px 15px;\n    }\n\n    .youtube-title {\n      font-size: 32px;\n    }\n\n    .youtube-subtitle {\n      font-size: 16px;\n    }\n\n    .youtube-videos-grid {\n      grid-template-columns: 1fr;\n      gap: 20px;\n    }\n\n    .youtube-modal-content {\n      width: 95%;\n      aspect-ratio: 16 / 9;\n    }\n\n    .youtube-modal-close {\n      top: -40px;\n      font-size: 30px;\n    }\n  }\n</style>\n\n<div class=\"youtube-section\">\n  <div class=\"youtube-container\">\n    <div class=\"youtube-header\">\n      <h2 class=\"youtube-title\">Unsere Videos</h2>\n      <p class=\"youtube-subtitle\">Entdecke die neuesten Nachrichten von unserem YouTube-Kanal</p>\n      <a href=\"https://www.youtube.com/@PflegeTeufel-Koeln\" target=\"_blank\" rel=\"noopener\" class=\"youtube-channel-link\">\n        <svg class=\"youtube-icon\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\n          <path d=\"M23.498 6.186a3.016 3.016 0 0 0-2.122-2.136C19.505 3.545 12 3.545 12 3.545s-7.505 0-9.377.505A3.017 3.017 0 0 0 .502 6.186C0 8.07 0 12 0 12s0 3.93.502 5.814a3.016 3.016 0 0 0 2.122 2.136c1.871.505 9.376.505 9.376.505s7.505 0 9.377-.505a3.015 3.015 0 0 0 2.122-2.136C24 15.93 24 12 24 12s0-3.93-.502-5.814zM9.545 15.568V8.432L15.818 12l-6.273 3.568z\"/>\n        </svg>\n        Besuche unseren Kanal\n      </a>\n    </div>\n\n    <div id=\"youtube-videos-container\" class=\"youtube-videos-grid\">\n      <div class=\"youtube-loading\">Videos werden geladen</div>\n    </div>\n  </div>\n</div>\n\n<!-- Modal per riprodurre video -->\n<div class=\"youtube-modal\" id=\"youtubeModal\">\n  <div class=\"youtube-modal-content\">\n    <button class=\"youtube-modal-close\" id=\"closeModal\" aria-label=\"Chiudi video\">&times;</button>\n    <iframe\n      class=\"youtube-modal-iframe\"\n      id=\"youtubeIframe\"\n      allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\"\n      allowfullscreen>\n    </iframe>\n  </div>\n</div>\n\n<script>\n(function() {\n  // CONFIGURAZIONE\n  const CHANNEL_ID = 'UCpfAr_rQA0UVtwr5hJn1cpw'; // Canale Pflege Teufel\n  const API_KEY = 'AIzaSyBn5qOR044wjeA9d7DN-mFGbiDY9Q2Ks7Q';\n  const MAX_RESULTS = 4; // Numero di video da mostrare\n\n  const container = document.getElementById('youtube-videos-container');\n\n  // Funktion zur Datumsformatierung\n  function formatDate(dateString) {\n    const date = new Date(dateString);\n    const now = new Date();\n    const diffTime = Math.abs(now - date);\n    const diffDays = Math.floor(diffTime / (1000 * 60 * 60 * 24));\n\n    if (diffDays === 0) return 'Heute';\n    if (diffDays === 1) return 'Gestern';\n    if (diffDays < 7) return 'vor ' + diffDays + ' Tagen';\n    if (diffDays < 30) return 'vor ' + Math.floor(diffDays / 7) + ' Wochen';\n    if (diffDays < 365) return 'vor ' + Math.floor(diffDays / 30) + ' Monaten';\n    return 'vor ' + Math.floor(diffDays / 365) + ' Jahren';\n  }\n\n  // Funzione per formattare le visualizzazioni\n  function formatViews(views) {\n    if (views >= 1000000) return (views / 1000000).toFixed(1) + 'M';\n    if (views >= 1000) return (views / 1000).toFixed(1) + 'K';\n    return views;\n  }\n\n  // Carica i video\n  async function loadVideos() {\n    try {\n      // Cerca i video del canale\n      const response = await fetch(\n        `https://www.googleapis.com/youtube/v3/search?key=${API_KEY}&channelId=${CHANNEL_ID}&part=snippet,id&order=date&maxResults=${MAX_RESULTS}&type=video`\n      );\n\n      if (!response.ok) {\n        throw new Error('Errore API: ' + response.status);\n      }\n\n      const data = await response.json();\n\n      if (!data.items || data.items.length === 0) {\n        container.innerHTML = '<div class=\"youtube-error\">Keine Videos auf dem Kanal gefunden</div>';\n        return;\n      }\n\n      // Ottieni le statistiche dei video\n      const videoIds = data.items.map(item => item.id.videoId).join(',');\n      const statsResponse = await fetch(\n        `https://www.googleapis.com/youtube/v3/videos?key=${API_KEY}&id=${videoIds}&part=statistics`\n      );\n      const statsData = await statsResponse.json();\n\n      // Crea le card dei video\n      container.innerHTML = data.items.map((item, index) => {\n        const videoId = item.id.videoId;\n        const title = item.snippet.title;\n        const thumbnail = item.snippet.thumbnails.high.url;\n        const publishedAt = item.snippet.publishedAt;\n        const views = statsData.items[index]?.statistics?.viewCount || '0';\n\n        return `\n          <div class=\"youtube-video-card\" onclick=\"openVideoModal('${videoId}')\">\n            <div class=\"youtube-video-thumbnail\">\n              <img src=\"${thumbnail}\" alt=\"${title}\" loading=\"lazy\">\n              <div class=\"youtube-play-overlay\">\n                <svg viewBox=\"0 0 24 24\">\n                  <path d=\"M8 5v14l11-7z\"/>\n                </svg>\n              </div>\n            </div>\n            <div class=\"youtube-video-info\">\n              <h3 class=\"youtube-video-title\">${title}</h3>\n              <div class=\"youtube-video-meta\">\n                <span>${formatViews(views)} Aufrufe</span>\n                <span>${formatDate(publishedAt)}</span>\n              </div>\n            </div>\n          </div>\n        `;\n      }).join('');\n\n    } catch (error) {\n      console.error('Fehler beim Laden der YouTube-Videos:', error);\n      container.innerHTML = '<div class=\"youtube-error\">Fehler beim Laden der Videos. Bitte versuche es sp√§ter erneut.</div>';\n    }\n  }\n\n  // Carica i video quando la pagina √® pronta\n  if (document.readyState === 'loading') {\n    document.addEventListener('DOMContentLoaded', loadVideos);\n  } else {\n    loadVideos();\n  }\n\n  // Gestione Modal\n  const modal = document.getElementById('youtubeModal');\n  const iframe = document.getElementById('youtubeIframe');\n  const closeBtn = document.getElementById('closeModal');\n\n  // Funzione per aprire il modal con il video\n  window.openVideoModal = function(videoId) {\n    iframe.src = `https://www.youtube.com/embed/${videoId}?autoplay=1&rel=0`;\n    modal.classList.add('active');\n    document.body.style.overflow = 'hidden'; // Blocca lo scroll della pagina\n  };\n\n  // Funzione per chiudere il modal\n  function closeModal() {\n    modal.classList.remove('active');\n    iframe.src = ''; // Ferma il video\n    document.body.style.overflow = ''; // Riabilita lo scroll\n  }\n\n  // Chiudi cliccando sulla X\n  closeBtn.addEventListener('click', closeModal);\n\n  // Chiudi cliccando fuori dal video\n  modal.addEventListener('click', function(e) {\n    if (e.target === modal) {\n      closeModal();\n    }\n  });\n\n  // Chiudi con il tasto ESC\n  document.addEventListener('keydown', function(e) {\n    if (e.key === 'Escape' && modal.classList.contains('active')) {\n      closeModal();\n    }\n  });\n})();\n</script>",
