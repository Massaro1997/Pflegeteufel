    "section_mGxhhk": {
      "type": "section",
      "blocks": {
        "custom_liquid_XiGUfV": {
          "type": "custom-liquid",
          "settings": {
            "custom_liquid": "<!DOCTYPE html>\n<html lang=\"de\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Sezione Orari - Layout Orizzontale</title>\n    <style>\n        /* WRAPPER ISOLAMENTO - ORARI */\n        .pflegebox-hours-wrapper {\n            font-family: inherit;\n            width: 100%;\n            margin: 0;\n            padding: 0;\n            box-sizing: border-box;\n        }\n\n        /* Reset interno per evitare conflitti */\n        .pflegebox-hours-wrapper * {\n            box-sizing: border-box;\n        }\n\n        .pflegebox-hours-wrapper h1,\n        .pflegebox-hours-wrapper h2,\n        .pflegebox-hours-wrapper h3,\n        .pflegebox-hours-wrapper h4,\n        .pflegebox-hours-wrapper h5,\n        .pflegebox-hours-wrapper h6,\n        .pflegebox-hours-wrapper p,\n        .pflegebox-hours-wrapper div,\n        .pflegebox-hours-wrapper span,\n        .pflegebox-hours-wrapper ul,\n        .pflegebox-hours-wrapper li {\n            margin: 0;\n            padding: 0;\n            border: 0;\n            font-size: inherit;\n            font-weight: inherit;\n            line-height: inherit;\n            color: inherit;\n            background: none;\n        }\n\n        /* 📏 SEZIONE STANDARDIZZATA */\n        .pflegebox-hours-wrapper .pflegebox-section {\n            padding: 20px 0;\n            background: #F8F8F8;\n        }\n\n        /* 📏 CONTAINER STANDARDIZZATO */\n        .pflegebox-hours-wrapper .pflegebox-section-container {\n            max-width: 1200px;\n            margin: 0 auto;\n            padding: 0 20px;\n            width: 100%;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-section-header {\n            text-align: center;\n            margin-bottom: 40px;\n            opacity: 0;\n            transform: translateY(30px);\n            transition: all 1.2s ease;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-section-header.animate {\n            opacity: 1;\n            transform: translateY(0);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-section-title {\n            font-size: 2.2rem;\n            font-weight: 700;\n            color: #2c3e50;\n            margin-bottom: 12px;\n            line-height: 1.2;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-section-subtitle {\n            font-size: 1.1rem;\n            color: #6c757d;\n            max-width: 700px;\n            margin: 0 auto;\n            line-height: 1.6;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-address-highlight {\n            color: #C12624;\n            font-weight: 700;\n            display: block;\n            margin-top: 15px;\n            font-size: 1.05em;\n        }\n\n        /* 📏 LAYOUT GRIGLIA 3 COLONNE */\n        .pflegebox-hours-wrapper .pflegebox-hours-grid {\n            display: grid;\n            grid-template-columns: repeat(3, 1fr);\n            gap: 20px;\n            margin-bottom: 40px;\n        }\n\n        /* 📏 CARD TUTTE UGUALI */\n        .pflegebox-hours-wrapper .pflegebox-hours-card {\n            background: white;\n            border-radius: 12px;\n            padding: 30px;\n            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n            border: 1px solid #e9ecef;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            text-align: center;\n            opacity: 0;\n            transform: translateY(50px);\n            transition: all 0.8s ease;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.animate {\n            opacity: 1;\n            transform: translateY(0);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card:hover {\n            transform: translateY(-5px);\n            box-shadow: 0 8px 30px rgba(0, 0, 0, 0.12);\n        }\n\n        /* 📏 ICONA CENTRATA */\n        .pflegebox-hours-wrapper .pflegebox-hours-icon {\n            background: white;\n            width: 80px;\n            height: 80px;\n            border-radius: 50%;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-bottom: 20px;\n            overflow: hidden;\n            position: relative;\n            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\n            border: 3px solid #f8f9fa;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-icon img {\n            width: 50px;\n            height: 50px;\n            object-fit: contain;\n            transition: all 0.3s ease;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card:hover .pflegebox-hours-icon img {\n            transform: scale(1.1);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card:hover .pflegebox-hours-icon {\n            transform: translateY(-3px);\n            box-shadow: 0 6px 20px rgba(0, 0, 0, 0.15);\n        }\n\n        /* 📏 CONTENUTO CENTRATO */\n        .pflegebox-hours-wrapper .pflegebox-hours-content {\n            width: 100%;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-title {\n            font-size: 1.3rem;\n            font-weight: 700;\n            color: #2c3e50;\n            margin-bottom: 15px;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-list {\n            list-style: none;\n            padding: 0;\n            margin: 0 0 15px 0;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-list li {\n            padding: 5px 0;\n            display: flex;\n            justify-content: space-between;\n            border-bottom: 1px solid #f0f0f0;\n            font-size: 0.95rem;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-list li:last-child {\n            border-bottom: none;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-day {\n            font-weight: 600;\n            color: #2c3e50;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-time {\n            color: #6c757d;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-contact-info {\n            color: #6c757d;\n            font-size: 0.9rem;\n            line-height: 1.5;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-contact-info a {\n            color: #2c3e50;\n            text-decoration: none;\n            font-weight: 600;\n            transition: color 0.3s ease;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-contact-info a:hover {\n            color: #C12624;\n        }\n\n        /* 📏 CARD NOTFALL */\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency {\n            background: linear-gradient(135deg, #C12624 0%, #A01F1D 100%);\n            color: white;\n            border: 2px solid #A01F1D;\n            position: relative;\n            overflow: hidden;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            text-align: center;\n            padding: 30px;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 0;\n            height: 4px;\n            background: linear-gradient(90deg, #A01F1D, #8B1A1A);\n            z-index: 1;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency:hover {\n            transform: translateY(-8px) scale(1.02);\n            box-shadow: 0 12px 40px rgba(193, 38, 36, 0.3);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-hours-title {\n            color: white;\n            font-weight: 800;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-day {\n            color: rgba(255, 255, 255, 0.95);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-time {\n            color: rgba(255, 255, 255, 0.9);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-hours-list li {\n            border-bottom: 1px solid rgba(255, 255, 255, 0.2);\n        }\n\n        /* 📏 ICONA EMERGENCY */\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-hours-icon {\n            background: white;\n            border: 3px solid rgba(255, 255, 255, 0.9);\n            backdrop-filter: blur(10px);\n            animation: pulse 2s infinite;\n            width: 80px;\n            height: 80px;\n            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);\n        }\n\n        @keyframes pulse {\n            0%, 100% { transform: scale(1); }\n            50% { transform: scale(1.1); }\n        }\n\n        /* 📏 NUMERO DI EMERGENZA */\n        .pflegebox-hours-wrapper .emergency-number {\n            background: rgba(255, 255, 255, 0.15);\n            color: white !important;\n            padding: 15px;\n            border-radius: 8px;\n            border: 2px solid rgba(255, 255, 255, 0.3);\n            backdrop-filter: blur(10px);\n            text-align: center;\n            margin-top: 15px;\n            width: 100%;\n        }\n\n        .pflegebox-hours-wrapper .emergency-number-label {\n            font-size: 0.9rem;\n            font-weight: 600;\n            margin-bottom: 8px;\n            text-transform: uppercase;\n            letter-spacing: 0.5px;\n            opacity: 0.9;\n        }\n\n        .pflegebox-hours-wrapper .emergency-number-phone {\n            font-size: 1.3rem;\n            font-weight: 800;\n            color: white !important;\n            text-decoration: none;\n            display: block;\n            transition: all 0.3s ease;\n            letter-spacing: 1px;\n        }\n\n        .pflegebox-hours-wrapper .emergency-number-phone:hover {\n            transform: scale(1.05);\n            color: #f8f9fa !important;\n            text-shadow: 0 2px 8px rgba(0, 0, 0, 0.3);\n        }\n\n        /* RESPONSIVE */\n        @media (max-width: 768px) {\n            .pflegebox-hours-wrapper .pflegebox-section {\n                padding: 40px 0;\n            }\n            \n            .pflegebox-hours-wrapper .pflegebox-section-container {\n                padding: 0 20px;\n            }\n            \n            .pflegebox-hours-wrapper .pflegebox-section-header {\n                margin-bottom: 40px;\n            }\n            \n            .pflegebox-hours-wrapper .pflegebox-section-title {\n                font-size: 1.8rem;\n            }\n\n            .pflegebox-hours-wrapper .pflegebox-section-subtitle {\n                max-width: 100%;\n            }\n\n            .pflegebox-hours-wrapper .pflegebox-address-highlight {\n                font-size: 1rem;\n                margin-top: 12px;\n            }\n            \n            .pflegebox-hours-wrapper .pflegebox-hours-grid {\n                grid-template-columns: 1fr;\n                gap: 20px;\n            }\n\n            .pflegebox-hours-wrapper .pflegebox-hours-card {\n                padding: 25px 20px;\n            }\n\n            .pflegebox-hours-wrapper .pflegebox-hours-card.emergency {\n                padding: 25px 20px;\n            }\n\n            .pflegebox-hours-wrapper .emergency-number {\n                width: 100%;\n            }\n\n            .pflegebox-hours-wrapper .emergency-number-phone {\n                font-size: 1.2rem;\n            }\n        }\n    </style>\n</head>\n<body>\n    <!-- SEZIONE ORARI - LAYOUT ORIZZONTALE -->\n    <div class=\"pflegebox-hours-wrapper\">\n        <section class=\"pflegebox-section\">\n            <div class=\"pflegebox-section-container\">\n                <div class=\"pflegebox-section-header\" id=\"hours-header\">\n                    <h2 class=\"pflegebox-section-title\">UNSERE ÖFFNUNGSZEITEN</h2>\n                    <p class=\"pflegebox-section-subtitle\">\n                        Wir sind für Sie da - kontaktieren Sie uns zu unseren Sprechzeiten oder schreiben Sie uns eine Nachricht.\n                        <strong class=\"pflegebox-address-highlight\">\n                            📍 Während unseren Öffnungszeiten können Sie uns auch in der Regentenstraße 88 besuchen in Köln Mülheim\n                        </strong>\n                    </p>\n                </div>\n                \n                <!-- 📏 3 CARD UGUALI IN GRIGLIA -->\n                <div class=\"pflegebox-hours-grid\">\n                    <!-- CARD 1: BUCHHALTUNG -->\n                    <div class=\"pflegebox-hours-card\" id=\"hours-card-1\">\n                        <div class=\"pflegebox-hours-icon\">\n                            <img src=\"https://img.icons8.com/color/96/phone.png\" alt=\"Telefon Icon\">\n                        </div>\n                        <div class=\"pflegebox-hours-content\">\n                            <h3 class=\"pflegebox-hours-title\">Buchhaltung</h3>\n                            <ul class=\"pflegebox-hours-list\">\n                                <li><span class=\"pflegebox-day\">Dienstag, Donnerstag, Freitag</span> <span class=\"pflegebox-time\">09:00 - 16:00</span></li>\n                            </ul>\n                            <div class=\"pflegebox-contact-info\">\n                                📞 <a href=\"tel:+4922142368489\">0221 - 4236 8489</a><br>\n                                👤 Paul\n                            </div>\n                        </div>\n                    </div>\n                    \n                    <!-- CARD 2: PFLEGEBOX / ASSISTENT GESCHÄFTSLEITUNG -->\n                    <div class=\"pflegebox-hours-card\" id=\"hours-card-2\">\n                        <div class=\"pflegebox-hours-icon\">\n                            <img src=\"https://img.icons8.com/color/96/business.png\" alt=\"Business Icon\">\n                        </div>\n                        <div class=\"pflegebox-hours-content\">\n                            <h3 class=\"pflegebox-hours-title\">Pflegebox / Assistent Geschäftsleitung</h3>\n                            <ul class=\"pflegebox-hours-list\">\n                                <li><span class=\"pflegebox-day\">Montag bis Freitag</span> <span class=\"pflegebox-time\">09:00 - 18:00</span></li>\n                            </ul>\n                            <div class=\"pflegebox-contact-info\">\n                                📞 <a href=\"tel:+4922142369757\">0221 - 4236 9757</a><br>\n                                👤 Luigi & Rosario\n                            </div>\n                        </div>\n                    </div>\n                    \n                    <!-- CARD 3: ITALIENISCH -->\n                    <div class=\"pflegebox-hours-card\" id=\"hours-card-3\">\n                        <div class=\"pflegebox-hours-icon\">\n                            <img src=\"https://img.icons8.com/color/96/italy.png\" alt=\"Italia Icon\">\n                        </div>\n                        <div class=\"pflegebox-hours-content\">\n                            <h3 class=\"pflegebox-hours-title\">Italienisch</h3>\n                            <ul class=\"pflegebox-hours-list\">\n                                <li><span class=\"pflegebox-day\">Montag und Mittwoch</span> <span class=\"pflegebox-time\">10:00 - 16:30</span></li>\n                            </ul>\n                            <div class=\"pflegebox-contact-info\">\n                                📞 <a href=\"tel:+4922142367062\">0221 - 4236 7062</a><br>\n                                👤 Salvatore\n                            </div>\n                        </div>\n                    </div>\n                </div>\n\n                <!-- SEZIONE EMERGENCY SEPARATA -->\n                <div class=\"pflegebox-hours-grid\" style=\"grid-template-columns: 1fr; max-width: 600px; margin: 0 auto;\">\n                    <div class=\"pflegebox-hours-card emergency\" id=\"hours-card-emergency\">\n                        <div class=\"pflegebox-hours-icon\">\n                            <img src=\"https://img.icons8.com/color/96/alarm.png\" alt=\"Emergency Alarm Icon\">\n                        </div>\n                        <div class=\"pflegebox-hours-content\">\n                            <h3 class=\"pflegebox-hours-title\">Notfall-Support</h3>\n                            <ul class=\"pflegebox-hours-list\">\n                                <li><span class=\"pflegebox-day\">Dringende Fälle</span> <span class=\"pflegebox-time\">24/7</span></li>\n                                <li><span class=\"pflegebox-day\">Pflegebox-Notfall</span> <span class=\"pflegebox-time\">Täglich</span></li>\n                                <li><span class=\"pflegebox-day\">Antragsfristen</span> <span class=\"pflegebox-time\">Sofort</span></li>\n                            </ul>\n                        </div>\n                        \n                        <div class=\"emergency-number\">\n                            <div class=\"emergency-number-label\">📱 Notfall-Nummer</div>\n                            <a href=\"tel:+4915224833381\" class=\"emergency-number-phone\">01522 4833381</a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </section>\n    </div>\n\n    <script>\n    // Funzione per osservare le sezioni Orari e animarle\n    function initHoursAnimations() {\n        var observer = new IntersectionObserver(function(entries) {\n            entries.forEach(function(entry) {\n                if (entry.isIntersecting) {\n                    entry.target.classList.add('animate');\n                }\n            });\n        }, { threshold: 0.1 });\n        \n        // Osserva tutti gli elementi da animare\n        var elementsToAnimate = [\n            'hours-header',\n            'hours-card-1',\n            'hours-card-2', \n            'hours-card-3',\n            'hours-card-emergency'\n        ];\n        \n        elementsToAnimate.forEach(function(id) {\n            var element = document.getElementById(id);\n            if (element) {\n                observer.observe(element);\n            }\n        });\n    }\n\n    // Animazioni sequenziali per le card orari\n    function animateHoursCards() {\n        var hoursCards = document.querySelectorAll('.pflegebox-hours-wrapper .pflegebox-hours-card');\n        hoursCards.forEach(function(card, index) {\n            card.style.animationDelay = (index * 300) + 'ms';\n        });\n    }\n\n    // Inizializzazione Orari\n    document.addEventListener('DOMContentLoaded', function() {\n        initHoursAnimations();\n        animateHoursCards();\n    });\n\n    if (document.readyState === 'loading') {\n        document.addEventListener('DOMContentLoaded', function() {\n            initHoursAnimations();\n            animateHoursCards();\n        });\n    } else {\n        initHoursAnimations();\n        animateHoursCards();\n    }\n    </script>\n</body>\n</html>"
