            "custom_liquid": "<!DOCTYPE html>\n<html lang=\"de\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Disclaimer Section - Z-Index Scalare: 10</title>\n    <style>\n        .disclaimer-section {\n            /* üìè MARGINI STANDARDIZZATI - come primo liquid */\n            padding: 40px 0;\n            background: #F8F8F8;\n            position: relative;\n            overflow: hidden;\n            font-family: 'Inter', sans-serif;\n            /* Z-INDEX SCALARE - SEZIONE PI√ô ALTA */\n            z-index: 10;\n            isolation: isolate;\n        }\n        \n        /* Effetto lucido bianco */\n        .disclaimer-section::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 0;\n            bottom: 0;\n            background: linear-gradient(135deg, \n                rgba(255, 255, 255, 0.1) 0%, \n                rgba(248, 249, 250, 0.3) 25%, \n                rgba(255, 255, 255, 0.2) 50%, \n                rgba(248, 249, 250, 0.1) 75%, \n                rgba(255, 255, 255, 0.15) 100%\n            );\n            pointer-events: none;\n            z-index: 1;\n        }\n        \n        /* Effetto brillantezza */\n        .disclaimer-section::after {\n            content: '';\n            position: absolute;\n            top: -50%;\n            left: -50%;\n            width: 200%;\n            height: 200%;\n            background: radial-gradient(\n                circle at center,\n                rgba(255, 255, 255, 0.1) 0%,\n                rgba(255, 255, 255, 0.05) 30%,\n                transparent 70%\n            );\n            animation: disclaimerShine 6s ease-in-out infinite;\n            pointer-events: none;\n            z-index: 2;\n        }\n        \n        @keyframes disclaimerShine {\n            0%, 100% { \n                transform: rotate(0deg) scale(1);\n                opacity: 0.3;\n            }\n            50% { \n                transform: rotate(180deg) scale(1.1);\n                opacity: 0.6;\n            }\n        }\n        \n        .disclaimer-container {\n            /* üìè CONTAINER STANDARDIZZATO - come primo liquid */\n            max-width: 1200px;\n            margin: 0 auto;\n            padding: 0 20px;\n            text-align: center;\n            position: relative;\n            z-index: 3;\n        }\n        \n        .disclaimer-card {\n            background: rgba(255, 255, 255, 0.95);\n            backdrop-filter: blur(15px);\n            border-radius: 12px;\n            /* üìè PADDING AUMENTATO per uguagliare i margini */\n            padding: 30px 40px;\n            box-shadow: \n                0 8px 32px rgba(0, 0, 0, 0.06),\n                0 2px 8px rgba(0, 0, 0, 0.03),\n                inset 0 1px 0 rgba(255, 255, 255, 1);\n            border: 1px solid rgba(255, 255, 255, 0.8);\n            position: relative;\n            overflow: hidden;\n            opacity: 0;\n            transform: translateY(30px);\n            transition: all 1.2s cubic-bezier(0.175, 0.885, 0.32, 1.275);\n            z-index: 1;\n            /* üìè LARGEZZA UGUALE AL CONTAINER STANDARD - come prima sezione */\n            width: 100%;\n            margin: 0 auto;\n        }\n        \n        .disclaimer-card.animate {\n            opacity: 1;\n            transform: translateY(0);\n        }\n        \n        /* Effetto lucido sulla card */\n        .disclaimer-card::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: -100%;\n            width: 100%;\n            height: 100%;\n            background: linear-gradient(\n                90deg,\n                transparent,\n                rgba(255, 255, 255, 0.4),\n                transparent\n            );\n            transition: left 1s ease;\n            z-index: 1;\n        }\n        \n        .disclaimer-card:hover::before {\n            left: 100%;\n        }\n        \n        .disclaimer-text {\n            font-size: 0.95rem;\n            color: #2c3e50;\n            line-height: 1.5;\n            margin: 0;\n            position: relative;\n            z-index: 2;\n            font-weight: 500;\n        }\n        \n        .disclaimer-text em {\n            font-style: italic;\n            color: #C12624;\n            font-weight: 700;\n            text-shadow: 0 1px 2px rgba(193, 38, 36, 0.1);\n        }\n        \n        .disclaimer-text strong {\n            font-weight: 800;\n            color: #1a252f;\n            text-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);\n        }\n        \n        /* Icona di avviso */\n        .disclaimer-icon {\n            display: inline-block;\n            width: 24px;\n            height: 24px;\n            background: linear-gradient(135deg, #C12624, #A01F1D);\n            border-radius: 50%;\n            margin-right: 8px;\n            position: relative;\n            vertical-align: middle;\n            box-shadow: 0 2px 8px rgba(193, 38, 36, 0.3);\n            z-index: 1;\n        }\n        \n        .disclaimer-icon::before {\n            content: '!';\n            position: absolute;\n            top: 50%;\n            left: 50%;\n            transform: translate(-50%, -50%);\n            color: white;\n            font-weight: bold;\n            font-size: 14px;\n            z-index: 1;\n        }\n        \n        /* REGOLA ANTI-POPUP */\n        body.popup-open .disclaimer-section {\n            z-index: 1 !important;\n        }\n        \n        body.popup-open .disclaimer-section * {\n            z-index: 1 !important;\n        }\n        \n        /* Responsive */\n        @media (max-width: 768px) {\n            .disclaimer-section {\n                /* üìè RESPONSIVE - mantiene proporzioni del primo liquid */\n                padding: 40px 0;\n            }\n            \n            .disclaimer-container {\n                padding: 0 20px;\n            }\n            \n            .disclaimer-card {\n                /* üìè RESPONSIVE - padding aumentato per uniformit√† */\n                padding: 30px 25px;\n                border-radius: 12px;\n            }\n            \n            .disclaimer-text {\n                font-size: 1rem;\n                line-height: 1.6;\n            }\n            \n            .disclaimer-icon {\n                width: 20px;\n                height: 20px;\n                margin-right: 6px;\n            }\n            \n            .disclaimer-icon::before {\n                font-size: 12px;\n            }\n        }\n        \n        /* DEBUG */\n        .disclaimer-section[data-debug=\"true\"] {\n            border: 3px dashed orange;\n        }\n        \n        .disclaimer-section[data-debug=\"true\"]::before {\n            content: 'Disclaimer - Z-Index: 10 (ALTO)';\n            position: absolute;\n            top: 5px;\n            left: 5px;\n            background: orange;\n            color: white;\n            padding: 3px 6px;\n            font-size: 11px;\n            z-index: 999;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"disclaimer-section\" id=\"disclaimer-section\">\n        <div class=\"disclaimer-container\">\n            <div class=\"disclaimer-card\" id=\"disclaimer-card\">\n                <p class=\"disclaimer-text\">\n                    <span class=\"disclaimer-icon\"></span>\n                    Die <em>Agentur Pflege Teufel</em> ist <strong>keine Pflegedienst</strong> und <strong>keine Rechtsanwaltskanzlei</strong>.<br>\n                    Wir bieten <strong>keine medizinische Pflege</strong>, <strong>keine ambulanten Pflegedienstleistungen</strong> und <strong>keine juristische Beratung</strong> an.\n                </p>\n            </div>\n        </div>\n    </div>\n\n    <script>\n    (function() {\n        'use strict';\n        \n        window.DisclaimerSection = window.DisclaimerSection || {};\n        \n        function disclaimerAnimate() {\n            setTimeout(function() {\n                var disclaimerCard = document.getElementById('disclaimer-card');\n                if (disclaimerCard) {\n                    disclaimerCard.classList.add('animate');\n                }\n            }, 300);\n        }\n        \n        function handlePopupOpen() {\n            document.body.classList.add('popup-open');\n            var disclaimerSection = document.getElementById('disclaimer-section');\n            if (disclaimerSection) {\n                disclaimerSection.style.zIndex = '1';\n            }\n        }\n        \n        function handlePopupClose() {\n            document.body.classList.remove('popup-open');\n            var disclaimerSection = document.getElementById('disclaimer-section');\n            if (disclaimerSection) {\n                disclaimerSection.style.zIndex = '10';\n            }\n        }\n        \n        function disclaimerInit() {\n            try {\n                disclaimerAnimate();\n                \n                if (window.location.search.includes('debug=true')) {\n                    document.getElementById('disclaimer-section').setAttribute('data-debug', 'true');\n                }\n                \n                console.log('‚úÖ Disclaimer Section - Z-Index: 10 (POSITION: TOP)');\n            } catch (error) {\n                console.log('Errore inizializzazione disclaimer:', error);\n            }\n        }\n        \n        window.DisclaimerSection.onPopupOpen = handlePopupOpen;\n        window.DisclaimerSection.onPopupClose = handlePopupClose;\n        window.DisclaimerSection.init = disclaimerInit;\n        \n        if (document.readyState === 'loading') {\n            document.addEventListener('DOMContentLoaded', disclaimerInit);\n        } else {\n            disclaimerInit();\n        }\n        \n        window.addEventListener('load', function() {\n            setTimeout(disclaimerInit, 100);\n        });\n        \n    })();\n    </script>\n</body>\n</html>"
