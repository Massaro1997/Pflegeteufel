       height: 180px;\n            object-fit: cover;\n            border-radius: 12px;\n            margin-bottom: 20px;\n            display: block;\n            margin-left: auto;\n            margin-right: auto;\n            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\n            transition: transform 0.3s ease;\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-image:hover {\n            transform: scale(1.05);\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-feature-list {\n            list-style: none;\n            padding: 0;\n            margin: 20px 0;\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-feature-list li {\n            padding: 8px 0;\n            display: flex;\n            align-items: center;\n            font-size: 1rem;\n            color: #555;\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-feature-list li::before {\n            content: \"‚úÖ\";\n            margin-right: 12px;\n            font-size: 16px;\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-highlight-text {\n            background: linear-gradient(45deg, #C12624, #A01F1D);\n            -webkit-background-clip: text;\n            -webkit-text-fill-color: transparent;\n            background-clip: text;\n            font-weight: 700;\n        }\n\n        /* BOTTONE ROSSO GRANDE E ILLUMINATO */\n        .pflegebox-professional-wrapper .pflegebox-order-button-container {\n            text-align: center;\n            margin-top: 40px;\n            opacity: 0;\n            transform: translateY(30px);\n            transition: all 1.2s ease;\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-order-button-container.animate {\n            opacity: 1;\n            transform: translateY(0);\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-order-button {\n            display: inline-block;\n            background: linear-gradient(135deg, #C12624, #A01F1D);\n            color: white;\n            padding: 22px 50px;\n            border-radius: 50px;\n            font-size: 1.5rem;\n            font-weight: bold;\n            text-transform: uppercase;\n            letter-spacing: 1.5px;\n            box-shadow: 0 8px 25px rgba(193, 38, 36, 0.5);\n            position: relative;\n            overflow: hidden;\n            transition: all 0.3s ease;\n            border: none;\n            cursor: pointer;\n            text-decoration: none;\n            animation: pflegeboxPulse 3s infinite;\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-order-button:hover {\n            transform: translateY(-3px) scale(1.05);\n            box-shadow: 0 12px 35px rgba(193, 38, 36, 0.6);\n        }\n\n        .pflegebox-professional-wrapper .pflegebox-order-button::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: -100%;\n            width: 100%;\n            height: 100%;\n            background: linear-gradient(90deg, transparent, rgba(255,255,255,0.3), transparent);\n            animation: pflegeboxShimmer 3s infinite;\n        }\n\n        @keyframes pflegeboxPulse {\n            0%, 100% { \n                box-shadow: 0 8px 25px rgba(193, 38, 36, 0.5),\n                            0 0 30px rgba(193, 38, 36, 0.3);\n            }\n            50% { \n                box-shadow: 0 8px 25px rgba(193, 38, 36, 0.5),\n                            0 0 50px rgba(193, 38, 36, 0.5),\n                            0 0 70px rgba(193, 38, 36, 0.3);\n                transform: scale(1.02);\n            }\n        }\n\n        @keyframes pflegeboxShimmer {\n            0% { left: -100%; }\n            50% { left: 100%; }\n            100% { left: 100%; }\n        }\n\n        /* RESPONSIVE */\n        @media (max-width: 768px) {\n            .pflegebox-professional-wrapper .pflegebox-grid {\n                grid-template-columns: 1fr;\n                gap: 30px;\n            }\n            \n            .pflegebox-professional-wrapper .pflegebox-section-title {\n                font-size: 1.8rem;\n            }\n            \n            .pflegebox-professional-wrapper .pflegebox-content,\n            .pflegebox-professional-wrapper .pflegebox-visual {\n                transform: translateY(50px);\n            }\n            \n            .pflegebox-professional-wrapper .pflegebox-content.animate,\n            .pflegebox-professional-wrapper .pflegebox-visual.animate {\n                transform: translateY(0);\n            }\n\n            .pflegebox-professional-wrapper .pflegebox-image {\n                width: 150px;\n                height: 150px;\n            }\n\n            .pflegebox-professional-wrapper .pflegebox-order-button {\n                font-size: 1.1rem;\n                padding: 18px 35px;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"pflegebox-professional-wrapper\">\n        <section class=\"pflegebox-section\">\n            <div class=\"pflegebox-section-container\">\n                <div class=\"pflegebox-section-header\" id=\"pflegebox-header\">\n                    <h2 class=\"pflegebox-section-title\">DIE PFLEGEBOX ‚Äì IHR MONATLICHES PAKET</h2>\n                    <p class=\"pflegebox-section-subtitle\">\n                        Hochwertige Pflegehilfsmittel direkt zu Ihnen nach Hause. Von der Pflegekasse bezahlt.\n                    </p>\n                </div>\n                \n                <div class=\"pflegebox-grid\">\n                    <div class=\"pflegebox-content\" id=\"pflegebox-content\">\n                        <h3 style=\"font-size: 1.5rem; color: #2c3e50; margin-bottom: 20px;\">\n                            Was ist die <span class=\"pflegebox-highlight-text\">Pflegebox</span>?\n                        </h3>\n                        <p style=\"font-size: 1.1rem; line-height: 1.6; color: #555; margin-bottom: 25px;\">\n                            Die Pflegebox ist Ihr monatliches Paket mit allen wichtigen Pflegehilfsmitteln zum Verbrauch. \n                            Perfekt abgestimmt auf Ihre Bed√ºrfnisse und direkt von der Pflegekasse finanziert.\n                        </p>\n                        \n                        <ul class=\"pflegebox-feature-list\">\n                            <li>Einmalhandschuhe in verschiedenen Gr√∂√üen</li>\n                            <li>Desinfektionsmittel f√ºr H√§nde und Fl√§chen</li>\n                            <li>Mundschutzmasken und Schutzsch√ºrzen</li>\n                            <li>Bettschutzeinlagen und Inkontinenzartikel</li>\n                            <li>Monatlich bis zu <strong>42‚Ç¨ kostenfrei</strong></li>\n                            <li>Lieferung direkt vor Ihre Haust√ºr</li>\n                        </ul>\n                        \n                        <div style=\"background: #fef7f7; padding: 20px; border-radius: 8px; border-left: 4px solid #C12624; margin-top: 25px;\">\n                            <strong style=\"color: #C12624;\">üí° Gut zu wissen:</strong>\n                            <p style=\"margin: 8px 0 0 0; color: #555; font-size: 0.95rem;\">\n                                Laut ¬ß 40 SGB XI haben alle Personen mit Pflegegrad 1-5 Anspruch auf diese Hilfsmittel. \n                                Wir k√ºmmern uns um die Antragstellung und Abrechnung mit Ihrer Pflegekasse.\n                            </p>\n                        </div>\n                    </div>\n                    \n                    <div class=\"pflegebox-visual\" id=\"pflegebox-visual\">\n                        <img src=\"https://cdn.shopify.com/s/files/1/0909/2805/4653/files/0624e619-4c93-462d-86b9-6bb2f218fa44_a764ab18-f1d9-42f9-949f-fd78265128c6.jpg?v=1751094187\"\n                             alt=\"PflegeBox Paket\" \n                             class=\"pflegebox-image\">\n                        <h4 style=\"font-size: 1.3rem; color: #2c3e50; margin-top: 30px; margin-bottom: 10px;\">Ihre monatliche Pflegebox</h4>\n                        <p style=\"color: #6c757d; margin-bottom: 20px;\">\n                            Individuell zusammengestellt nach Ihren Bed√ºrfnissen\n                        </p>\n                        <div style=\"background: white; padding: 20px; border-radius: 8px; margin-top: 20px;\">\n                            <div style=\"display: flex; justify-content: space-between; align-items: center; margin-bottom: 10px;\">\n                                <span style=\"font-weight: 600;\">Warenwert:</span>\n                                <span style=\"color: #C12624; font-weight: 700;\">bis 42‚Ç¨</span>\n                            </div>\n                            <div style=\"display: flex; justify-content: space-between; align-items: center; margin-bottom: 10px;\">\n                                <span style=\"font-weight: 600;\">Ihre Kosten:</span>\n                                <span style=\"color: #C12624; font-weight: 700; font-size: 1.2rem;\">0‚Ç¨*</span>\n                            </div>\n                            <small style=\"color: #6c757d;\">*Bei Pflegegrad 1-5 und h√§uslicher Pflege</small>\n                        </div>\n                    </div>\n                </div>\n                \n                <!-- BOTTONE CENTRATO FUORI DALLA CARD -->\n                <div class=\"pflegebox-order-button-container\" id=\"pflegebox-button\">\n                    <a href=\"https://pflegeteufel.de/collections/all\" class=\"pflegebox-order-button\">\n                        Jetzt bestellen\n                    </a>\n                </div>\n            </div>\n        </section>\n    </div>\n\n    <script>\n        function initPflegeboxProfessionalAnimations() {\n            const observer = new IntersectionObserver((entries) => {\n                entries.forEach((entry) => {\n                    if (entry.isIntersecting) {\n                        entry.target.classList.add('animate');\n                    }\n                });\n            }, { threshold: 0.1 });\n            \n            const elementsToAnimate = [\n                'pflegebox-header',\n                'pflegebox-content', \n                'pflegebox-visual',\n                'pflegebox-button'\n            ];\n            \n            elementsToAnimate.forEach((id) => {\n                const element = document.getElementById(id);\n                if (element) {\n                    observer.observe(element);\n                }\n            });\n        }\n\n        if (document.readyState === 'loading') {\n            document.addEventListener('DOMContentLoaded', initPflegeboxProfessionalAnimations);\n        } else {\n            initPflegeboxProfessionalAnimations();\n        }\n    </script>\n</body>\n</html>"
