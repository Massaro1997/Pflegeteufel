card:hover::before {\n            left: 100%;\n        }\n        \n        .disclaimer-text {\n            font-size: 0.95rem;\n            color: #2c3e50;\n            line-height: 1.5;\n            margin: 0;\n            position: relative;\n            z-index: 2;\n            font-weight: 500;\n        }\n        \n        .disclaimer-text em {\n            font-style: italic;\n            color: #C12624;\n            font-weight: 700;\n            text-shadow: 0 1px 2px rgba(193, 38, 36, 0.1);\n        }\n        \n        .disclaimer-text strong {\n            font-weight: 800;\n            color: #1a252f;\n            text-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);\n        }\n        \n        /* Icona di avviso */\n        .disclaimer-icon {\n            display: inline-block;\n            width: 24px;\n            height: 24px;\n            background: linear-gradient(135deg, #C12624, #A01F1D);\n            border-radius: 50%;\n            margin-right: 8px;\n            position: relative;\n            vertical-align: middle;\n            box-shadow: 0 2px 8px rgba(193, 38, 36, 0.3);\n            z-index: 1;\n        }\n        \n        .disclaimer-icon::before {\n            content: '!';\n            position: absolute;\n            top: 50%;\n            left: 50%;\n            transform: translate(-50%, -50%);\n            color: white;\n            font-weight: bold;\n            font-size: 14px;\n            z-index: 1;\n        }\n        \n        /* REGOLA ANTI-POPUP */\n        body.popup-open .disclaimer-section {\n            z-index: 1 !important;\n        }\n        \n        body.popup-open .disclaimer-section * {\n            z-index: 1 !important;\n        }\n        \n        /* Responsive */\n        @media (max-width: 768px) {\n            .disclaimer-section {\n                /* 📏 RESPONSIVE - mantiene proporzioni del primo liquid */\n                padding: 40px 0;\n            }\n            \n            .disclaimer-container {\n  