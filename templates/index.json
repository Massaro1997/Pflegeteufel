            "custom_liquid": "<!DOCTYPE html>\n<html lang=\"de\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Pflege Teufel - Besuchen Sie uns</title>\n</head>\n<body>\n\n<div class=\"pflegeteufel-address-wrapper\">\n<style>\n    /* Reset completo e isolamento */\n    .pflegeteufel-address-wrapper {\n        all: initial;\n        display: block;\n        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;\n        isolation: isolate;\n        contain: layout style;\n        line-height: 1.6;\n        color: #333;\n        padding: 0;\n        background: #F8F8F8;\n        min-height: 100vh;\n    }\n\n    .pflegeteufel-address-wrapper *,\n    .pflegeteufel-address-wrapper *::before,\n    .pflegeteufel-address-wrapper *::after {\n        box-sizing: border-box;\n        margin: 0;\n        padding: 0;\n    }\n\n    /* Container principale */\n    .pflegeteufel-address-wrapper .main-container {\n        max-width: 1600px;\n        margin: 0 auto;\n        padding: 40px 60px;\n        width: 100%;\n    }\n\n    /* Card dell'indirizzo */\n    .pflegeteufel-address-wrapper .address-card {\n        background: transparent;\n        padding: 30px;\n        border-radius: 0;\n    }\n\n    /* Titolo principale */\n    .pflegeteufel-address-wrapper .main-title {\n        font-size: 3rem;\n        font-weight: 800;\n        color: #C12624;\n        text-align: center;\n        margin-bottom: 20px;\n        letter-spacing: -0.02em;\n    }\n\n    /* Sezione indirizzo */\n    .pflegeteufel-address-wrapper .address-info {\n        text-align: center;\n        margin-bottom: 25px;\n    }\n\n    .pflegeteufel-address-wrapper .street-address {\n        font-size: 2.2rem;\n        font-weight: 700;\n        color: #2c3e50;\n        margin-bottom: 8px;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        gap: 12px;\n    }\n\n    .pflegeteufel-address-wrapper .city-info {\n        font-size: 1.6rem;\n        color: #6c757d;\n        font-weight: 600;\n    }\n\n    /* Container mappa */\n    .pflegeteufel-address-wrapper .map-wrapper {\n        margin-bottom: 25px;\n    }\n\n    .pflegeteufel-address-wrapper .interactive-map {\n        position: relative;\n        cursor: pointer;\n        border-radius: 16px;\n        overflow: hidden;\n        box-shadow: 0 8px 32px rgba(0,0,0,0.12);\n        transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);\n        background: #f8f9fa;\n    }\n\n    .pflegeteufel-address-wrapper .interactive-map:hover {\n        transform: translateY(-8px);\n        box-shadow: 0 16px 48px rgba(0,0,0,0.18);\n    }\n\n    .pflegeteufel-address-wrapper .map-image {\n        width: 100%;\n        height: 700px;\n        object-fit: cover;\n        display: block;\n        transition: opacity 0.3s ease;\n    }\n\n    .pflegeteufel-address-wrapper .map-image:hover {\n        opacity: 0.95;\n    }\n\n    .pflegeteufel-address-wrapper .map-action-button {\n        position: absolute;\n        bottom: 30px;\n        right: 30px;\n        background: linear-gradient(135deg, #C12624, #A01F1D);\n        color: white;\n        padding: 16px 28px;\n        border-radius: 30px;\n        font-size: 16px;\n        font-weight: 700;\n        box-shadow: 0 6px 20px rgba(193, 38, 36, 0.4);\n        transition: all 0.3s ease;\n        border: 2px solid transparent;\n    }\n\n    .pflegeteufel-address-wrapper .map-action-button:hover {\n        transform: translateY(-2px);\n        box-shadow: 0 8px 28px rgba(193, 38, 36, 0.5);\n        border-color: rgba(255,255,255,0.2);\n    }\n\n    /* Sezione call-to-action */\n    .pflegeteufel-address-wrapper .cta-wrapper {\n        text-align: center;\n    }\n\n    .pflegeteufel-address-wrapper .cta-link {\n        display: inline-block;\n        background: linear-gradient(135deg, #C12624, #A01F1D);\n        color: white;\n  