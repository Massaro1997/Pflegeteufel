ection || {};\n        \n        function disclaimerAnimate() {\n            setTimeout(function() {\n                var disclaimerCard = document.getElementById('disclaimer-card');\n                if (disclaimerCard) {\n                    disclaimerCard.classList.add('animate');\n                }\n            }, 300);\n        }\n        \n        function handlePopupOpen() {\n            document.body.classList.add('popup-open');\n            var disclaimerSection = document.getElementById('disclaimer-section');\n            if (disclaimerSection) {\n                disclaimerSection.style.zIndex = '1';\n            }\n        }\n        \n        function handlePopupClose() {\n            document.body.classList.remove('popup-open');\n            var disclaimerSection = document.getElementById('disclaimer-section');\n            if (disclaimerSection) {\n                disclaimerSection.style.zIndex = '10';\n            }\n        }\n        \n        function disclaimerInit() {\n            try {\n                disclaimerAnimate();\n                \n                if (window.location.search.includes('debug=true')) {\n                    document.getElementById('disclaimer-section').setAttribute('data-debug', 'true');\n                }\n                \n                console.log('✅ Disclaimer Section - Z-Index: 10 (POSITION: TOP)');\n            } catch (error) {\n                console.log('Errore inizializzazione disclaimer:', error);\n            }\n        }\n        \n        window.DisclaimerSection.onPopupOpen = handlePopupOpen;\n        window.DisclaimerSection.onPopupClose = handlePopupClose;\n        window.DisclaimerSection.init = disclaimerInit;\n        \n        if (document.readyState === 'loading') {\n            document.addEventListener('DOMContentLoaded', disclaimerInit);\n        } else {\n            disclaimerInit();\n        }\n        \n        window.addEventListener('load', function() {\n            setTimeout(disclaimerInit, 100);\n        });\n        \n    })();\n    </script>\n</body>\n</html>"
