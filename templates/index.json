    "section_mGxhhk": {
      "type": "section",
      "blocks": {
        "custom_liquid_XiGUfV": {
          "type": "custom-liquid",
          "settings": {
            "custom_liquid": "<!DOCTYPE html>\n<html lang=\"de\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Sezione Orari - Layout Orizzontale</title>\n    <style>\n        /* WRAPPER ISOLAMENTO - ORARI */\n        .pflegebox-hours-wrapper {\n            font-family: inherit;\n            width: 100%;\n            margin: 0;\n            padding: 0;\n            box-sizing: border-box;\n        }\n\n        /* Reset interno per evitare conflitti */\n        .pflegebox-hours-wrapper * {\n            box-sizing: border-box;\n        }\n\n        .pflegebox-hours-wrapper h1,\n        .pflegebox-hours-wrapper h2,\n        .pflegebox-hours-wrapper h3,\n        .pflegebox-hours-wrapper h4,\n        .pflegebox-hours-wrapper h5,\n        .pflegebox-hours-wrapper h6,\n        .pflegebox-hours-wrapper p,\n        .pflegebox-hours-wrapper div,\n        .pflegebox-hours-wrapper span,\n        .pflegebox-hours-wrapper ul,\n        .pflegebox-hours-wrapper li {\n            margin: 0;\n            padding: 0;\n            border: 0;\n            font-size: inherit;\n            font-weight: inherit;\n            line-height: inherit;\n            color: inherit;\n            background: none;\n        }\n\n        /* üìè SEZIONE STANDARDIZZATA */\n        .pflegebox-hours-wrapper .pflegebox-section {\n            padding: 20px 0;\n            background: #F8F8F8;\n        }\n\n        /* üìè CONTAINER STANDARDIZZATO */\n        .pflegebox-hours-wrapper .pflegebox-section-container {\n            max-width: 1200px;\n            margin: 0 auto;\n            padding: 0 20px;\n            width: 100%;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-section-header {\n            text-align: center;\n            margin-bottom: 40px;\n            opacity: 0;\n            transform: translateY(30px);\n            transition: all 1.2s ease;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-section-header.animate {\n            opacity: 1;\n            transform: translateY(0);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-section-title {\n            font-size: 2.2rem;\n            font-weight: 700;\n            color: #2c3e50;\n            margin-bottom: 12px;\n            line-height: 1.2;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-section-subtitle {\n            font-size: 1.1rem;\n            color: #6c757d;\n            max-width: 700px;\n            margin: 0 auto;\n            line-height: 1.6;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-address-highlight {\n            color: #C12624;\n            font-weight: 700;\n            display: block;\n            margin-top: 15px;\n            font-size: 1.05em;\n        }\n\n        /* üìè LAYOUT GRIGLIA 3 COLONNE */\n        .pflegebox-hours-wrapper .pflegebox-hours-grid {\n            display: grid;\n            grid-template-columns: repeat(3, 1fr);\n            gap: 20px;\n            margin-bottom: 40px;\n        }\n\n        /* üìè CARD TUTTE UGUALI */\n        .pflegebox-hours-wrapper .pflegebox-hours-card {\n            background: white;\n            border-radius: 12px;\n            padding: 30px;\n            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n            border: 1px solid #e9ecef;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            text-align: center;\n            opacity: 0;\n            transform: translateY(50px);\n            transition: all 0.8s ease;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.animate {\n            opacity: 1;\n            transform: translateY(0);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card:hover {\n            transform: translateY(-5px);\n            box-shadow: 0 8px 30px rgba(0, 0, 0, 0.12);\n        }\n\n        /* üìè ICONA CENTRATA */\n        .pflegebox-hours-wrapper .pflegebox-hours-icon {\n            background: white;\n            width: 80px;\n            height: 80px;\n            border-radius: 50%;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-bottom: 20px;\n            overflow: hidden;\n            position: relative;\n            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\n            border: 3px solid #f8f9fa;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-icon img {\n            width: 50px;\n            height: 50px;\n            object-fit: contain;\n            transition: all 0.3s ease;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card:hover .pflegebox-hours-icon img {\n            transform: scale(1.1);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card:hover .pflegebox-hours-icon {\n            transform: translateY(-3px);\n            box-shadow: 0 6px 20px rgba(0, 0, 0, 0.15);\n        }\n\n        /* üìè CONTENUTO CENTRATO */\n        .pflegebox-hours-wrapper .pflegebox-hours-content {\n            width: 100%;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-title {\n            font-size: 1.3rem;\n            font-weight: 700;\n            color: #2c3e50;\n            margin-bottom: 15px;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-list {\n            list-style: none;\n            padding: 0;\n            margin: 0 0 15px 0;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-list li {\n            padding: 5px 0;\n            display: flex;\n            justify-content: space-between;\n            border-bottom: 1px solid #f0f0f0;\n            font-size: 0.95rem;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-list li:last-child {\n            border-bottom: none;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-day {\n            font-weight: 600;\n            color: #2c3e50;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-time {\n            color: #6c757d;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-contact-info {\n            color: #6c757d;\n            font-size: 0.9rem;\n            line-height: 1.5;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-contact-info a {\n            color: #2c3e50;\n            text-decoration: none;\n            font-weight: 600;\n            transition: color 0.3s ease;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-contact-info a:hover {\n            color: #C12624;\n        }\n\n        /* üìè CARD NOTFALL */\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency {\n            background: linear-gradient(135deg, #C12624 0%, #A01F1D 100%);\n            color: white;\n            border: 2px solid #A01F1D;\n            position: relative;\n            overflow: hidden;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            text-align: center;\n            padding: 30px;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 0;\n            height: 4px;\n            background: linear-gradient(90deg, #A01F1D, #8B1A1A);\n            z-index: 1;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency:hover {\n            transform: translateY(-8px) scale(1.02);\n            box-shadow: 0 12px 40px rgba(193, 38, 36, 0.3);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-hours-title {\n            color: white;\n            font-weight: 800;\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-day {\n            color: rgba(255, 255, 255, 0.95);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-time {\n            color: rgba(255, 255, 255, 0.9);\n        }\n\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-hours-list li {\n            border-bottom: 1px solid rgba(255, 255, 255, 0.2);\n        }\n\n        /* üìè ICONA EMERGENCY */\n        .pflegebox-hours-wrapper .pflegebox-hours-card.emergency .pflegebox-hours-icon {\n            background: white;\n            border: 3px solid rgba(255, 255, 255, 0.9);\n            backdrop-filter: blur(10px);\n            animation: pulse 2s infinite;\n            width: 80px;\n            height: 80px;\n            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);\n        }\n\n        @keyframes pulse {\n            0%, 100% { transform: scale(1); }\n            50% { transform: scale(1.1); }\n        }\n\n        /* üìè NUMERO DI EMERGENZA */\n        .pflegebox-hours-wrapper .emergency-number {\n            background: rgba(255, 255, 255, 0.15);\n            color: white !important;\n            padding: 15px;\n            border-radius: 8px;\n            border: 2px solid rgba(255, 255, 255, 0.3);\n            backdrop-filter: blur(10px);\n            text-align: center;\n            margin-top: 15px;\n            width: 100%;\n        }\n\n        .pflegebox-hours-wrapper .emergency-number-label {\n            font-size: 0.9rem;\n            font-weight: 600;\n            margin-bottom: 8px;\n            text-transform: uppercase;\n            letter-spacing: 0.5px;\n            opacity: 0.9;\n        }\n\n        .pflegebox-hours-wrapper .emergency-number-phone {\n            font-size: 1.3rem;\n            font-weight: 800;\n            color: white !important;\n            text-decoration: none;\n            display: block;\n            transition: all 0.3s ease;\n            letter-spacing: 1px;\n        }\n\n        .pflegebox-hours-wrapper .emergency-number-phone:hover {\n            transform: scale(1.05);\n            color: #f8f9fa !important;\n            text-shadow: 0 2px 8px rgba(0, 0, 0, 0.3);\n        }\n\n        /* RESPONSIVE